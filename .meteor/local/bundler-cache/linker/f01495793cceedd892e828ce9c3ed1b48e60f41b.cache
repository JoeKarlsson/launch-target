plate.registerHelper('thisMoment', function() {\n   return (((moment().hours() * 60) + moment().minutes()) * 60) + moment().seconds();\n});\n\nTemplate.setTimeTemplate.helpers({\n  inTimeSwitchSelected : function() {\n    return Session.get('inTimeSwitchSelected');\n  }\n});\n\n\n//init Session to inTime show\nTemplate.setTimeTemplate.created = function() {\n  Session.set('inTimeSwitchSelected', true);\n}\n\nTemplate.setTimeTemplate.events({\n  'click #inTimeSwitch' : function() {\n    Session.set('inTimeSwitchSelected', true)\n  },\n  'click #atTimeSwitch' : function() {\n    Session.set('inTimeSwitchSelected', false)\n  }\n});\n\nTemplate.inTimeTemplate.rendered = function() {\n  $('#dtBox').DateTimePicker({\n    minTime : '00:15',\n    maxTime : '04:00',\n    minuteInterval : 5,\n    setValueInTextboxOnEveryClick : true\n  });\n};\n\nTemplate.inTimeTemplate.events({\n  'click .deploy' : function(event, template) {\n    event.preventDefault();\n    var now = UI._globalHelpers.thisMoment();\n    var hrs = ($('.intime').val().substring(0, 2));\n    var mins = ($('.intime').val().substring(3, 5));\n    var inputIn = ((Number(hrs) * 60) + Number(mins)) * 60\n    Launches.update({_id : this._id}\n      , { $set : { lunchtime : (inputIn + now)} });\n    Router.go('finalLaunchPlan', { _id : this._id });\n  }\n})\n\n\nTemplate.atTimeTemplate.rendered = function() {\n  $('#dtBox').DateTimePicker({\n    minuteInterval : 5,\n    setValueInTextboxOnEveryClick : true,\n    timeFormat : 'hh:mm AA',\n    isPopup : false\n  });\n};\n\nTemplate.atTimeTemplate.events({\n  'click .deploy' : function(event, template) {\n    event.preventDefault();\n    var now = UI._globalHelpers.thisMoment();\n    var ampm = $('.attime').val().substring(6, 8);\n    var hrs = ($('.attime').val().substring(0, 2));\n    var mins = ($('.attime').val().substring(3, 5));\n    if (ampm == 'PM') {\n      hrs = Number(hrs) + 12;\n    };\n    var launchTarget = (((Number(hrs) * 60) + Number(mins)) * 60);\n    Launches.update({_id : this._id},\n      { $set : { lunchtime : launchTarget} });\n    var inputIn = launchTarget - now;\n    if(inputIn < 900 || inputIn > 14400) {\n      alert('please plan between 15 minutes and 4 hours');\n    } else {\n      Router.go('finalLaunchPlan', { _id : this._id });\n   }\n  }\n})"]}},{"type":"js","data":"(function(){\n\n/////////////////////////////////////////////////////////////////////////\n//                                                                     //\n// client/views/03-set-timer/timePicker.js                             //\n//                                                                     //\n/////////////////////////////////////////////////////////////////////////\n                                                                       //\nTemplate.pickerTemplate.rendered = function () {                       // 1\n  $('.hourValue').html('00');                                          // 2\n  $('.minuteValue').html('15');                                        // 3\n};                                                                     //\n                                                                       //\nTemplate.pickerTemplate.onRendered(function () {                       // 6\n  setInterval(function () {                                            // 7\n                                                                       //\n    var pickedHourValue = parseInt($('.hourValue').html());            // 9\n    var pickedMinuteValue = parseInt($('.minuteValue').html());        // 10\n    var currentHourValue = moment().hours() + pickedHourValue;         // 11\n    var currentMinuteValue = moment().minutes() + pickedMinuteValue;   // 12\n    var convertedHourValue;                                            // 13\n    var convertedMinuteValue;                                          // 14\n    var amPm;                                                          // 15\n    var timeString;                                                    // 16\n                                                                       //\n    if (currentMinuteValue >= 60) {                                    // 18\n      currentHourValue++;                                              // 19\n      currentMinuteValue -= 60;                                        // 20\n    }                                                                  //\n    if (currentMinuteValue <= 9) {                                     // 22\n      convertedMinuteValue = '0' + currentMinuteValue.toString();      // 23\n    } else {                                                           //\n      convertedMinuteValue = currentMinuteValue.toString();            // 25\n    }                                                                  //\n                                                                       //\n    if (currentHourValue >= 24) {                                      // 28\n      currentHourValue = 0;                                            // 29\n    }                                                                  //\n                                                                       //\n    if (currentHourValue > 12) {                                       // 32\n      convertedHourValue = currentHourValue - 12;                      // 33\n      amPm = 'PM';                                                     // 34\n    } else if (currentHourValue == 12) {                               //\n      convertedHourValue = currentHourValue;                           // 36\n      amPm = 'PM';                                                     // 37\n    } else {                                                           //\n      convertedHourValue = currentHourValue;                           // 39\n      amPm = 'AM';                                                     // 40\n    }                                                                  //\n                                                                       //\n    this.$('.timeString').html(convertedHourValue + ':' + convertedMinuteValue + ' ' + amPm);\n  }, 100);                                                             //\n});                                                                    //\n                                                                       //\nTemplate.pickerTemplate.events({                                       // 48\n  /*                                                                   //\n    * ######## HOUR EVENTS #########                                   //\n    *                                                                  //\n  */                                                                   //\n  'click .incrementHour': function (event, template) {                 // 53\n    event.preventDefault();                                            // 54\n                                                                       //\n    // parse the current value                                         //\n    var value = parseInt($('.hourValue').html());                      // 57\n                                                                       //\n    // cap on the amount of time                                       //\n    if (value < 3) {                                                   // 60\n      value++;                                                         // 61\n      $('.hourValue').html('0' + value.toString());                    // 62\n    } else if (value == 3) {                                           //\n      value++;                                                         // 64\n      $('.hourValue').html('04');                                      // 65\n      $('.minuteValue').html('00');                                    // 66\n    }                                                                  //\n  },                                                                   //\n  'click .decrementHour': function (event, template) {                 // 69\n    event.preventDefault();                                            // 70\n                                                                       //\n    // parse the current value                                         //\n    var value = parseInt($('.hourValue').html());                      // 73\n                                                                       //\n    // cap on the least amount of time                                 //\n    if (value > 1) {                                                   // 76\n      value--;                                                         // 77\n      $('.hourValue').html('0' + value.toString());                    // 78\n    } else {                                                           //\n      value--;                                                         // 80\n      $('.hourValue').html('00');                                      // 81\n      $('.minuteValue').html('15');                                    // 82\n    }                                                                  //\n  },                                                                   //\n  /*                                                                   //\n    * ######### MINUTE EVENTS ##########                               //\n    *                                                                  //\n  */                                                                   //\n  'click .incrementMinute': function (event, template) {               // 89\n    event.preventDefault();                                            // 90\n                                                                       //\n    // parse the current value                                         //\n    var value = parseInt($('.minuteValue').html());                    // 93\n    var hourValue = parseInt($('.hourValue').html());                  // 94\n                                                                       //\n    // cap on the amount of time                                       //\n    if (value < 59 && hourValue !== 4) {                               // 97\n      value++;                                                         // 98\n                                                                       //\n      // if its a single digit append 0                                //\n      if (value < 10) {                                                // 101\n        $('.minuteValue').html('0' + value.toString());                // 102\n      } else {                                                         //\n        $('.minuteValue').html(value.toString());                      // 104\n      }                                                                //\n    } else if (hourValue <= 3) {                                       //\n      hourValue++;                                                     // 107\n      $('.hourValue').html(hourValue.toString());                      // 108\n      $('.minuteValue').html('00');                                    // 109\n    }                                                                  //\n  },                                                                   //\n  'click .decrementMinute': function (event, template) {               // 112\n    event.preventDefault();                                            // 113\n                                                                       //\n    // parse the current value                                         //\n    var value = parseInt($('.minuteValue').html());                    // 116\n    var hourValue = parseInt($('.hourValue').html());                  // 117\n                                                                       //\n    // cap on the amount of time to minimum 15 minutes                 //\n    if (hourValue == 0 && value > 15) {                                // 120\n      value--;                                                         // 121\n                                                                       //\n      $('.minuteValue').html(value.toString());                        // 123\n                                                                       //\n      // if there is an hour value, then we can decrement              //\n    } else if (hourValue > 0 && value >= 0) {                          //\n                                                                       //\n        value--;                                                       // 128\n                                                                       //\n        // if its greater than double digits (15 to 10)                //\n        if (value > 9) {                                               // 131\n                                                                       //\n          // just print the value                                      //\n          $('.minuteValue').html(value.toString());                    // 134\n                                                                       //\n          // else if its between 10 and 0, then append a 0             //\n        } else if (value > 0 && value < 10) {                          //\n            $('.minuteValue').html('0' + value.toString());            // 138\n          } else if (value <= 0) {                                     //\n                                                                       //\n            // else, then we can minus an hour                         //\n            hourValue--;                                               // 142\n            $('.minuteValue').html('59');                              // 143\n            $('.hourValue').html(hourValue.toString());                // 144\n          }                                                            //\n      }                                                                //\n  },                                                                   //\n  'click .setTimeLaunch': function (event, template) {                 // 149\n    event.preventDefault();                                            // 150\n    var now = UI._globalHelpers.thisMoment();                          // 151\n    var hrs = $('.hourValue').html();                                  // 152\n    var mins = $('.minuteValue').html();                               // 153\n    var inputIn = (Number(hrs) * 60 + Number(mins)) * 60;              // 154\n    var launchtime = inputIn + now;                                    // 155\n    Launches.update({ _id: this._id }, { $set: { lunchtime: launchtime } });\n    Router.go('finalLaunchPlan', { _id: this._id });                   // 158\n  }                                                                    //\n});                                                                    //\n/////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n","servePath":"/client/views/03-set-timer/timePicker.js","sourceMap":{"version":3,"sources":["meteor://ðŸ’»app/client/views/03-set-timer/timePicker.js"],"names":[],"mappings":";;;;;;;;AAAA,QAAQ,CAAC,cAAc,CAAC,QAAQ,GAAG,YAAW;AAC5C,GAAC,CAAC,YAAY,CAAC,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC;AAC3B,GAAC,CAAC,cAAc,CAAC,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC;CAC9B,CAAC;;AAEF,QAAQ,CAAC,cAAc,CAAC,UAAU,CAAC,YAAY;AAC7C,aAAW,CAAC,YAAW;;AAErB,QAAI,eAAe,GAAG,QAAQ,CAAC,CAAC,CAAC,YAAY,CAAC,CAAC,IAAI,EAAE,CAAC,CAAC;AACvD,QAAI,iBAAiB,GAAG,QAAQ,CAAC,CAAC,CAAC,cAAc,CAAC,CAAC,IAAI,EAAE,CAAC,CAAC;AAC3D,QAAI,gBAAgB,GAAG,MAAM,EAAE,CAAC,KAAK,EAAE,GAAG,eAAe,CAAC;AAC1D,QAAI,kBAAkB,GAAG,MAAM,EAAE,CAAC,OAAO,EAAE,GAAG,iBAAiB,CAAC;AAChE,QAAI,kBAAkB,CAAC;AACvB,QAAI,oBAAoB,CAAC;AACzB,QAAI,IAAI,CAAC;AACT,QAAI,UAAU,CAAC;;AAEf,QAAI,kBAAkB,IAAI,EAAE,EAAE;AAC5B,sBAAgB,EAAE,CAAC;AACnB,wBAAkB,IAAI,EAAE,CAAC;KAC1B;AACD,QAAI,kBAAkB,IAAI,CAAC,EAAE;AAC3B,0BAAoB,GAAG,GAAG,GAAG,kBAAkB,CAAC,QAAQ,EAAE,CAAC;KAC5D,MAAM;AACL,0BAAoB,GAAG,kBAAkB,CAAC,QAAQ,EAAE,CAAC;KACtD;;AAED,QAAI,gBAAgB,IAAI,E