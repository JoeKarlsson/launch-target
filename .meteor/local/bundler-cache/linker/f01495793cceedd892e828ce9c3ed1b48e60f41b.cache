aggable: false,                                              // 95\n        icon: place.icon,                                              // 96\n        animation: google.maps.Animation.DROP                          // 97\n      });                                                              //\n                                                                       //\n      var infoWindowContent = '<h3>' + place.name + '</h3><p> Votes: ' + place.votes + '</p>';\n                                                                       //\n      //event listener that loads resturant information into infowindow.\n      google.maps.event.addListener(marker, 'click', function () {     // 104\n        if (marker.getAnimation() !== null) {                          // 105\n          marker.setAnimation(null);                                   // 106\n        } else {                                                       //\n          marker.setAnimation(google.maps.Animation.BOUNCE);           // 108\n        }                                                              //\n        infowindow.setContent(infoWindowContent);                      // 110\n        infowindow.open(map.instance, this);                           // 111\n      });                                                              //\n      return marker;                                                   // 113\n    }                                                                  //\n  });                                                                  //\n});                                                                    //\n/////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n","servePath":"/client/views/04-final-launch-plan/final-launch-plan.js","sourceMap":{"version":3,"sources":["meteor://ðŸ’»app/client/views/04-final-launch-plan/final-launch-plan.js"],"names":[],"mappings":";;;;;;;;AAAA,IAAI,YAAY,CAAC;AACjB,IAAI,QAAQ,GAAG,EAAE,CAAC;AAClB,IAAI,OAAO,CAAC