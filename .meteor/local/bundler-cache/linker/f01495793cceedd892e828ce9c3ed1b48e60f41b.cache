 currentHourValue = moment().hours() + pickedHourValue;\n    var currentMinuteValue = moment().minutes() + pickedMinuteValue;\n    var convertedHourValue;\n    var convertedMinuteValue;\n    var amPm;\n    var timeString;\n\n    if (currentMinuteValue >= 60) {\n      currentHourValue++;\n      currentMinuteValue -= 60;\n    }\n    if (currentMinuteValue <= 9) {\n      convertedMinuteValue = '0' + currentMinuteValue.toString();\n    } else {\n      convertedMinuteValue = currentMinuteValue.toString();\n    }\n\n    if (currentHourValue >= 24) {\n      currentHourValue = 0;\n    }\n\n    if (currentHourValue > 12) {\n      convertedHourValue = currentHourValue - 12;\n      amPm = 'PM';\n    } else if (currentHourValue == 12) {\n      convertedHourValue = currentHourValue;\n      amPm = 'PM';\n    } else {\n      convertedHourValue = currentHourValue;\n      amPm = 'AM';\n    }\n\n    this.$('.timeString').html(convertedHourValue + ':' + convertedMinuteValue + ' ' + amPm);\n\n  }, 100);\n});\n\nTemplate.pickerTemplate.events({\n  /*\n    * ######## HOUR EVENTS #########\n    *\n  */\n  'click .incrementHour' : function(event, template) {\n    event.preventDefault();\n\n    // parse the current value\n    var value = parseInt($('.hourValue').html());\n\n    // cap on the amount of time\n    if (value < 3) {\n      value++;\n      $('.hourValue').html( '0'+ value.toString());\n    } else if (value == 3) {\n      value++;\n      $('.hourValue').html( '04' )\n      $('.minuteValue').html( '00' );\n    }\n  },\n  'click .decrementHour' : function(event, template) {\n    event.preventDefault();\n\n    // parse the current value\n    var value = parseInt($('.hourValue').html());\n\n    // cap on the least amount of time\n    if (value > 1) {\n      value--;\n      $('.hourValue').html( '0'+ value.toString());\n    } else {\n      value--;\n      $('.hourValue').html( '00' );\n      $('.minuteValue').html( '15' );\n    }\n  },\n  /*\n    * ######### MINUTE EVENTS ##########\n    *\n  */\n  'click .incrementMinute' : function(event, template) {\n    event.preventDefault();\n\n    // parse the current value\n    var value = parseInt($('.minuteValue').html());\n    var hourValue = parseInt($('.hourValue').html());\n\n    // cap on the amount of time\n    if (value < 59 && hourValue !== 4) {\n      value++;\n\n      // if its a single digit append 0\n      if (value < 10) {\n        $('.minuteValue').html( '0'+ value.toString() );\n      } else {\n        $('.minuteValue').html( value.toString() );\n      }\n    } else if (hourValue <= 3){\n      hourValue++;\n      $('.hourValue').html( hourValue.toString() );\n      $('.minuteValue').html( '00' );\n    }\n  },\n  'click .decrementMinute' : function(event, template) {\n    event.preventDefault();\n\n    // parse the current value\n    var value = parseInt($('.minuteValue').html());\n    var hourValue = parseInt($('.hourValue').html());\n\n    // cap on the amount of time to minimum 15 minutes\n    if (hourValue == 0 && value > 15) {\n      value--;\n\n      $('.minuteValue').html( value.toString() );\n\n    // if there is an hour value, then we can decrement\n    } else if (hourValue > 0 && value >= 0) {\n\n      value--;\n\n      // if its greater than double digits (15 to 10)\n      if (value > 9) {\n\n        // just print the value\n        $('.minuteValue').html( value.toString() );\n\n        // else if its between 10 and 0, then append a 0\n      } else if (value > 0 && value < 10) {\n          $('.minuteValue').html( '0' + value.toString() );\n      } else if (value <= 0) {\n\n        // else, then we can minus an hour\n          hourValue--;\n          $('.minuteValue').html( '59' );\n          $('.hourValue').html( hourValue.toString() );\n      }\n\n    }\n  },\n  'click .setTimeLaunch' : function(event, template) {\n    event.preventDefault();\n    var now = UI._globalHelpers.thisMoment();\n    var hrs = $('.hourValue').html();\n    var mins = $('.minuteValue').html();\n    var inputIn = ((Number(hrs) * 60) + Number(mins)) * 60\n    var launchtime = inputIn + now;\n    Launches.update({_id : this._id}\n      , { $set : { lunchtime : launchtime } });\n    Router.go('finalLaunchPlan', { _id : this._id });\n  }\n});"]}},{"type":"js","data":"(function(){\n\n/////////////////////////////////////////////////////////////////////////\n//                                                                     //\n// client/views/04-final-launch-plan/final-launch-list-item.js         //\n//                                                                     //\n/////////////////////////////////////////////////////////////////////////\n                                                                       //\nvar uberClientId = 'I8iDf46DcbPEC32PbibfPoBYBoHewzt8',                 // 1\n    uberServerToken = 'YOUR_SERVER_TOKEN';                             //\n                                                                       //\n// Create variables to store latitude and longitude                    //\nvar userLatitude, userLongitude, barLatitude, barLongitude;            // 5\n                                                                       //\n// Create variable to store timer                                      //\nvar timer;                                                             // 11\n                                                                       //\nvar planId;                                                            // 13\nvar voted = undefined;                                                 // 14\n                                                                       //\nTemplate.finalTargetListItem.helpers({                                 // 16\n  getPlanId: function () {                                             // 17\n    // reactive getParams method which will invalidate the comp if any part of the params change\n    // including the hash.                                             //\n    var controller = Iron.controller();                                // 20\n    var params = controller.getParams();                               // 21\n    planId = params._id;                                               // 22\n  }                                                                    //\n});                                                                    //\n                                                                       //\nTemplate.finalTargetListItem.events({                                  // 26\n  'click .ride_btn': function (event, template) {                      // 27\n    // Redirect to Uber API via deep-linking to the mobile web-app     //\n    var uberURL = 'https://m.uber.com/sign-up?';                       // 29\n                                                                       //\n    // Add parameters                                                  //\n    uberURL += 'client_id=' + uberClientId;                            // 32\n    if (typeof userLatitude != typeof undefined) uberURL += '&' + 'pickup_latitude=' + userLatitude;\n    if (typeof userLongitude != typeof undefined) uberURL += '&' + 'pickup_longitude=' + userLongitude;\n    uberURL += '&' + 'dropoff_latitude=' + barLatitude;                // 35\n    uberURL += '&' + 'dropoff_longitude=' + barLongitude;              // 36\n    uberURL += '&' + 'dropoff_nickname=' + 'Bar';                      // 37\n                                                                       //\n    // Redirect to Uber                                                //\n    window.location.href = uberURL;                                    // 40\n  }                                                                    //\n});                                                                    //\n                                                                       //\n//Voting for an item                                                   //\nTemplate.finalTargetListItem.events({                                  // 45\n  'click .collection-item': function (event, template) {               // 46\n    //initialize this to self                                          //\n    var self = this;                                                   // 48\n                                                                       //\n    if (voted === undefined) {                                         // 50\n      voteForItem();                                                   // 51\n    } else {                                                           //\n      overrideVote();                                                  // 53\n    }                                                                  //\n                                                                       //\n    function voteForItem() {                                           // 56\n      //Get all of the final selected launches that have been saved to the session\n      var allSelectedLaunches = Session.get('selectedLaunches');       // 58\n                                                                       //\n      //Find the index of the currently selected launch item in the DB Array\n      var index = allSelectedLaunches.findIndex(function (element) {   // 61\n        return element.placeId == self.placeId;                        // 61\n      });                                                              //\n                                                                       //\n      //Currently selected bar                                         //\n      var currentBar = allSelectedLaunches[index];                     // 64\n                                                                       //\n      //Get the total votes                                            //\n      var totalVotes = currentBar.votes;                               // 67\n      totalVotes++;                                                    // 68\n      currentBar.votes = totalVotes;                                   // 69\n      allSelectedLaunches[index] = currentBar;                         // 70\n                                                                       //\n      //Move the element with the most votes to the top of the list    //\n      var mostVotes = currentBar;                                      // 73\n      for (var i = 0; i < allSelectedLaunches.length; i++) {           // 74\n        if (allSelectedLaunches[i].votes > currentBar.votes) {         // 75\n          mostVotes = allSelectedLaunches[i];                          // 76\n          var indexMostVotes = allSelectedLaunches.findIndex(function (element) {\n            return element.placeId == mostVotes.placeId;               // 77\n          });                                                          //\n          allSelectedLaunches.splice(indexMostVotes, 1);               // 78\n          allSelectedLaunches.unshift(mostVotes);                      // 79\n        }                                                              //\n      };                                                               //\n                                                                       //\n      //Update the whole final selected launches in the DB             //\n      Launches.update(planId, { $set: { finalLaunches: allSelectedLaunches } }, function (error) {\n        if (error) {                                                   // 85\n          // display the error to the user                             //\n          alert(error.reason);                                         // 87\n        } else {                                                       //\n          console.log('Vote Added to ' + currentBar.name);             // 89\n        }                                                              //\n      });                                                              //\n                                                                       //\n      //Materialize toast when item is voted on                        //\n      // Materialize.toast('Voted for ' + currentBar.name, 4000);      //\n      voted = currentBar;                                              // 95\n    }                                                                  //\n                                                                       //\n    function overrideVote() {                                          // 99\n                                                                       //\n      var allSelectedLaunches = Session.get('selectedLaunches');       // 101\n      console.log(allSelectedLaunches, 'allSelectedLaunches');         // 102\n                                                                       //\n      //Find the index of the currently selected launch item in the DB Array\n      var indexNew = allSelectedLaunches.findIndex(function (element) {\n        return element.placeId == self.placeId;                        // 105\n      });                                                              //\n                                                                       //\n      var indexOld = allSelectedLaunches.findIndex(function (element) {\n        return element.placeId == voted.placeId;                       // 107\n      });                                                              //\n                                                                       //\n      //Currently selected bar                                         //\n      var currentBar = allSelectedLaunches[indexNew];                  // 110\n                                                                       //\n      var oldBar = allSelectedLaunches[indexOld];                      // 112\n                                                                       //\n      //Get the total votes for the current bar                        //\n      var totalVotesCurrent = currentBar.votes;                        // 115\n      totalVotesCurrent++;                                             // 116\n      currentBar.votes = totalVotesCurrent;                            // 117\n      allSelectedLaunches[indexNew] = currentBar;                      // 118\n                                                                       //\n      //remove the vote for the previously selected vote               //\n      var totalVotesOld = oldBar.votes;                                // 121\n      totalVotesOld--;                                                 // 122\n      oldBar.votes = totalVotesOld;                                    // 123\n      allSelectedLaunches[indexOld] = oldBar;                          // 124\n                                                                       //\n      //Move the element with the most votes to the top of the list    //\n      var mostVotes = currentBar;                                      // 127\n      for (var i = 0; i < allSelectedLaunches.length; i++) {           // 128\n        if (allSelectedLaunches[i].votes > currentBar.votes) {         // 129\n          mostVotes = allSelectedLaunches[i];                          // 130\n          var indexMostVotes = allSelectedLaunches.findIndex(function (element) {\n            return element.placeId == mostVotes.placeId;               // 131\n          });                                                          //\n          allSelectedLaunches.splice(indexMostVotes, 1);               // 132\n          allSelectedLaunches.unshift(mostVotes);                      // 133\n        }                                                              //\n      };                                                               //\n                                                                       //\n      //Update the whole final selected launches in the DB             //\n      Launches.update(planId, { $set: { finalLaunches: allSelectedLaunches } }, function (error) {\n        if (error) {                                                   // 139\n          // display the error to the user                             //\n          alert(error.reason);                                         // 141\n        } else {                                                       //\n          console.log('Vote Added to ' + currentBar.name);             // 143\n        }                                                              //\n      });                                                              //\n                                                                       //\n      //Materialize toast when item is voted on                        //\n      // Materialize.toast('Voted for ' + currentBar.name, 4000);      //\n      voted = currentBar;                                              // 149\n    }                                                                  //\n  }                                                                    //\n});                                                                    //\n/////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n","servePath":"/client/views/04-final-launch-plan/final-launch-list-item.js","sourceMap":{"version":3,"sources":["meteor://ðŸ’»app/client/views/04-final-launch-plan/final-launch-list-item.js"],"names":[],"mappings":";;;;;;;;AAAA,IAAI,YAAY,GAAG,kCAAkC;IAChD,eAAe,GAAG,mBAAmB,CAAC;;;AAG3C,IAAI,YAAY,EACZ,aAAa,EACb,WAAW,EACX,YAAY,CAAC;;;AAGjB,IAAI,KAAK,CAAC;;AAEV,IAAI,MAAM,CAAC;AACX,IAAI,KAAK,GAAG,SAAS,CAAC;;AAEtB,QAAQ,CAAC,mBAAmB,CAAC,OAAO,CAAC;AACnC,WAAS,EAAG,YAAW;;;AAGrB,QAAI,UAAU,GAAG,IAAI,CAAC,UAAU,EAAE,CAAC;AACnC,QAAI,MAAM,GAAG,UAAU,CAAC,SAAS,EAAE,CAAC;AACpC,UAAM,GAAG,MAAM,CAAC,GAAG,CAAC;GACrB;CACF,CAAC,CAAC;;AAEH,QAAQ,CAAC,mBAAmB,CAAC,MAAM,CAAC;AAClC,mBAAiB,EAAG,UAAU,KAAK,EAAE,QAAQ,EAAE;;AAE7C,QAAI,OAAO,GAAG,6BAA6B,CAAC;;;AAG5C,WAAO,IAAI,YAAY,GAAG,YAAY,CAAC;AACvC,QAAI,OAAO,YAAY,IAAI,OAAO,SAAS,EAAE,OAAO,IAAI,GAAG,GAAG,kBAAkB,GAAG,YAAY,CAAC;AAChG,QAAI,OAAO,aAAa,IAAI,OAAO,SAAS,EAAE,OAAO,IAAI,GAAG,GAAG,mBAAmB,GAAG,aAAa,CAAC;