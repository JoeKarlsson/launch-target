nt();                          // 40\n    var hrs = $('.intime').val().substring(0, 2);                      // 41\n    var mins = $('.intime').val().substring(3, 5);                     // 42\n    var inputIn = (Number(hrs) * 60 + Number(mins)) * 60;              // 43\n    Launches.update({ _id: this._id }, { $set: { lunchtime: inputIn + now } });\n    Router.go('finalLaunchPlan', { _id: this._id });                   // 46\n  }                                                                    //\n});                                                                    //\n                                                                       //\nTemplate.atTimeTemplate.rendered = function () {                       // 51\n  $('#dtBox').DateTimePicker({                                         // 52\n    minuteInterval: 5,                                                 // 53\n    setValueInTextboxOnEveryClick: true,                               // 54\n    timeFormat: 'hh:mm AA',                                            // 55\n    isPopup: false                                                     // 56\n  });                                                                  //\n};                                                                     //\n                                                                       //\nTemplate.atTimeTemplate.events({                                       // 60\n  'click .deploy': function (event, template) {                        // 61\n    event.preventDefault();                                            // 62\n    var now = UI._globalHelpers.thisMoment();                          // 63\n    var ampm = $('.attime').val().substring(6, 8);                     // 64\n    var hrs = $('.attime').val().substring(0, 2);                      // 65\n    var mins = $('.attime').val().substring(3, 5);                     // 66\n    if (ampm == 'PM') {                                                // 67\n      hrs = Number(hrs) + 12;                                          // 68\n    };                                                                 //\n    var launchTarget = (Number(hrs) * 60 + Number(mins)) * 60;         // 70\n    Launches.update({ _id: this._id }, { $set: { lunchtime: launchTarget } });\n    var inputIn = launchTarget - now;                                  // 73\n    if (inputIn < 900 || inputIn > 14400) {                            // 74\n      alert('please plan between 15 minutes and 4 hours');             // 75\n    } else {                                                           //\n      Router.go('finalLaunchPlan', { _id: this._id });                 // 77\n    }                                                                  //\n  }                                                                    //\n});                                                                    //\n/////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n","servePath":"/client/views/03-set-timer/set-timer.js","sourceMap":{"version":3,"sources":["meteor://ðŸ’»app/client/views/03-set-timer/set-timer.js"],"names":[],"mappings":";;;;;;;;;;AAEA,QAAQ,CAAC,cAAc,CAAC,YAAY,EAAE,YAAW;AAC9C,SAAQ,CAAE,MAAM,EAAE,CAAC,KAAK,EAAE,GAAG,EAAE,GAAI,MAAM,EAAE,CAAC,OAAO,EAAE,IAAI,EAAE,GAAI,MAAM,EAAE,CAAC,OAAO,EAAE,CAAC;CACpF,CAAC,CAAC;;AAEH,QAAQ,CAAC,eAAe,CAAC,OAAO,CAAC;AAC/B,sBAAoB,EAAG,YAAW;AAChC,WAAO,OAAO,CAAC,GAAG,CAAC,sBAAsB,CAAC,CAAC;GAC5C;CACF,CAAC,CAAC;;;AAIH,QAAQ,CAAC,eAAe,CAAC,OAAO,GAAG,YAAW;AAC5C,SAAO,CAAC,GAAG,CAAC,sBAAsB,EAAE,IAAI,CAAC,CAAC;CAC3C;;AAED,QAAQ,CAAC,eAAe,CAAC,MAAM,CAAC;AAC9B,uBAAqB,EAAG,YAAW;AACjC,WAAO,CAAC,GAAG,CAAC,sBAAsB,EAAE,IAAI,CAAC;GAC1C;AACD,uBAAqB,EAAG,YAAW;AACjC,WAAO,CAAC,GAAG,CAAC,sBAAsB,EAAE,KAAK,CAAC;GAC3C;CACF,CAAC,CAAC;;AAEH,QAAQ,CAAC,cAAc,CAAC,QAAQ,GAAG,YAAW;AAC5C,GAAC,CAAC,QAAQ,CAAC,CAAC,cAAc,CAAC;AACzB,WAAO,EAAG,OAAO;AACjB,WAAO,EAAG,OAAO;AACjB,kBAAc,EAAG,CAAC;AAClB,iCAA6B,EAAG,IAAI;GACrC,CAAC,CAAC;CACJ,CAAC;;AAEF,QAAQ,CAAC,c